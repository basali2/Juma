{
  "setupFiles": ["<rootDir>/test/setEnvVars.ts"],
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "modulePaths": [
    "<rootDir>"
  ],
  "testEnvironment": "node",
  "testMatch": [
    "**/src/**/*.e2e-spec.(ts)",
    "**/src/**/*.spec.(ts)"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
